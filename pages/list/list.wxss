/* pages/list/list.wxss */
.mainpart{
  height: 200px;
  width: 100%;
  text-align: center;
  line-height: 200px;
  display: flex;
  justify-content: space-around;
  border: 1px solid black;
}
.mainpart view:nth-child(1){
  background-color: rgb(103, 238, 148);
}
.mainpart view:nth-child(2){
  background-color: rgb(213, 250, 113);
}
.mainpart view:nth-child(3){
  background-color: rgb(46, 141, 121);
}
.swiper_top{
  height: 150px;
}
.mp_item{
  height: 100%;
  text-align: center;
  line-height: 150px;
  background-color: rgb(248, 247, 189);
}
image{
  border:1px solid red;
}